{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    },
    "applicationName": "grunt-readme-plus",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "grunt-readme-plus",
      "description": "",
      "keyword": ""
    },
    "linenums": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "tags": [
      "examples"
    ]
  }
}
